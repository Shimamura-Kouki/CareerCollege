NULL　ヤバい
NOT NULL


A = 'a' 
B = 'b'
C = NULL

（A = 'a') and (B = 'b') true
（A = 'a') and C         NULL
(A = 'a') or  C         true
C         or  (A = 'a') NULL
（A = 'b') and C         false



