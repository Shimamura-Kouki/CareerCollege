SELECT  *  FROM  users_1 WHERE user_id = 3;
SELECT  *  FROM  users_1 WHERE age = 19;

SELECT  *  FROM  int_1;
SELECT  *  FROM  int_1  WHERE  num  >  50  ;

SELECT  *  FROM  users_1 WHERE age != 19;
SELECT  *  FROM  users_1 WHERE age <> 19;

SELECT  *  FROM  int_1  ORDER BY  num;
SELECT  *  FROM  int_1  ORDER BY  num  DESC;

SELECT  *  FROM  int_1  WHERE  num  >=  50  ORDER BY  num;

